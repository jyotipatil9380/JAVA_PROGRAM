package Java_Programs;

public class Missing_Number {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]= {2,3,5,4};
		int n=arr.length;
		int s=0;
		int num=(n+1)*(n+2)/2;
		for(int i=0;i<n;i++)
		{
			s+=arr[i];
		}
		int mn=num-s;
		System.out.println(mn);
	}

}
